{
    "name": "kiwi/contao-designer",
    "type": "contao-bundle",
    "keywords": ["contao", "design", "colors", "colorschemes", "linkdesigns", "buttons", "backgrounds", "topline", "subline"],
    "description": "This bundle enables you to manage design settings and their assignment to contents within Contao CMS (e.g. colors and colorschemes, backgrounds and linkdesigns).",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Katrin Zimmermann",
            "homepage": "https://github.com/diekatrin"
        },
        {
            "name": "KIWI. Werbeagentur GmbH",
            "homepage": "https://www.kiwi.de"
        }
    ],
    "require": {
        "php": "^8.3",
        "contao/core-bundle": "^5.3",
        "kiwi/contao-cmx": "^1.0"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0"
    },
    "conflict": {
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "autoload": {
        "psr-4" : {
            "Kiwi\\Contao\\DesignerBundle\\" : "src/"
        }
    },
    "extra":{
        "contao-manager-plugin": "Kiwi\\Contao\\DesignerBundle\\ContaoManager\\Plugin"
    }
}